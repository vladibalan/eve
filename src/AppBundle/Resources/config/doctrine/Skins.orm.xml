<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Skins" table="skins">
    <id name="skinid" type="integer" column="skinID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="internalname" type="string" column="internalName" length="70" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="skinmaterialid" type="integer" column="skinMaterialID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
