<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Planetschematicstypemap" table="planetSchematicsTypeMap">
    <indexes>
      <index name="schematicID" columns="schematicID"/>
      <index name="typeID" columns="typeID"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="quantity" type="integer" column="quantity" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isinput" type="boolean" column="isInput" nullable="true"/>
    <many-to-one field="typeid" target-entity="Invtypes" fetch="LAZY">
      <join-columns>
        <join-column name="typeID" referenced-column-name="typeID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="schematicid" target-entity="Planetschematics" fetch="LAZY">
      <join-columns>
        <join-column name="schematicID" referenced-column-name="schematicID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
