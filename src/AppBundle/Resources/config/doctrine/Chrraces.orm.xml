<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Chrraces" table="chrRaces">
    <indexes>
      <index name="iconID" columns="iconID"/>
    </indexes>
    <id name="raceid" type="integer" column="raceID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="racename" type="string" column="raceName" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="shortdescription" type="string" column="shortDescription" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="iconid" target-entity="Eveicons" fetch="LAZY">
      <join-columns>
        <join-column name="iconID" referenced-column-name="iconID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
