<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Ramactivities" table="ramActivities">
    <id name="activityid" type="integer" column="activityID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="activityname" type="string" column="activityName" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iconno" type="string" column="iconNo" length="5" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="published" type="boolean" column="published" nullable="true"/>
  </entity>
</doctrine-mapping>
