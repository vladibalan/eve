<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Dgmattributecategories" table="dgmAttributeCategories">
    <id name="categoryid" type="integer" column="categoryID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="categoryname" type="string" column="categoryName" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categorydescription" type="string" column="categoryDescription" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
