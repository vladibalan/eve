<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Crpnpcdivisions" table="crpNPCDivisions">
    <id name="divisionid" type="integer" column="divisionID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="divisionname" type="string" column="divisionName" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="1000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="leadertype" type="string" column="leaderType" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
